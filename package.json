{
  "name": "fpish",
  "version": "4.0.0",
  "description": "FP-friendly classes for Typescript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=14.17.0"
  },
  "private": false,
  "scripts": {
    "build": "tsc -p tsconfig.production.json",
    "release": "standard-version",
    "prerelease": "yarn run prepare",
    "lint": "eslint -c .eslintrc.js --ext .ts src",
    "lint:ci": "eslint -c .eslintrc.js --ext .ts --format junit --output-file ./reports/eslint.xml src",
    "postversion": "git push && git push --tags",
    "prettier:cli": "prettier \"src/**/*.ts\"",
    "prettier:check": "yarn run prettier:cli -- -l",
    "prettier:fix": "yarn run prettier:cli -- --write",
    "prepare": "yarn run build",
    "prepublishOnly": "yarn run lint",
    "preversion": "yarn run lint",
    "test": "jest"
  },
  "keywords": [
    "FP",
    "Typescript"
  ],
  "author": "Dan Rumney <dancrumb@gmail.com>",
  "license": "MIT",
  "repository": "github:dancrumb/fpish",
  "devDependencies": {
    "@types/chai": "4.3.3",
    "@types/chai-as-promised": "7.1.5",
    "@types/jest": "^29.0.3",
    "@types/node": "18.7.21",
    "@types/sinon": "10.0.13",
    "@types/sinon-chai": "3.2.8",
    "@typescript-eslint/eslint-plugin": "5.38.0",
    "@typescript-eslint/parser": "5.38.0",
    "chai": "4.3.6",
    "chai-as-promised": "7.1.1",
    "eslint": "8.24.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "git-cz": "4.9.0",
    "jest": "^29.0.3",
    "prettier": "2.7.1",
    "sinon": "14.0.0",
    "sinon-chai": "3.7.0",
    "ts-jest": "^29.0.2",
    "ts-node": "10.9.1",
    "typescript": "4.8.3"
  },
  "files": [
    "dist/**/*"
  ],
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  }
}
