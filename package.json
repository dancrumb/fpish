{
  "name": "@dancrumb/fpish",
  "version": "5.2.1",
  "description": "FP-friendly classes for Typescript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=14.17.0"
  },
  "scripts": {
    "build": "yarn run tsc -p tsconfig.production.json",
    "prerelease": "yarn run prepare",
    "lint": "yarn run eslint -c .eslintrc.js --ext .ts src",
    "lint:ci": "yarn run eslint -c .eslintrc.js --ext .ts --format junit --output-file ./reports/eslint.xml src",
    "postversion": "git push && git push --tags",
    "prettier:cli": "yarn run prettier \"src/**/*.ts\"",
    "prettier:check": "yarn run prettier:cli -- -l",
    "prettier:fix": "yarn run prettier:cli -- --write",
    "prepack": "yarn run build",
    "prepublish": "yarn run lint",
    "preversion": "yarn run lint",
    "test": "yarn run jest"
  },
  "keywords": [
    "FP",
    "Typescript"
  ],
  "author": "Dan Rumney <dancrumb@gmail.com>",
  "license": "MIT",
  "repository": "github:dancrumb/fpish",
  "devDependencies": {
    "@jest/globals": "^29.6.2",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.7.2",
    "@typescript-eslint/eslint-plugin": "6.2.1",
    "@typescript-eslint/parser": "6.2.1",
    "eslint": "8.46.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.0",
    "git-cz": "4.9.0",
    "jest": "^29.6.2",
    "prettier": "3.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "10.9.1",
    "typescript": "^5.1.6"
  },
  "files": [
    "dist/**/*"
  ],
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "packageManager": "yarn@3.6.1"
}
